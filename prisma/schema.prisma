generator client {
  provider = "prisma-client-js"

}
datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model todoItems {
  id          Int     @id @default(autoincrement())
  title       String  @db.VarChar(255)
  description String  @db.Text
  status      String  @default("pending") @db.VarChar(50)
}